{
  "name": "climate-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run config:dev && vite",
    "build": "npm run config:prod && tsc -b && vite build",
    "build:dev": "npm run config:dev && tsc -b && vite build",
    "build:int": "npm run config:int && tsc -b && vite build",
    "build:prod": "npm run config:prod && tsc -b && vite build",
    "config:dev": "node scripts/build-config.js DEV",
    "config:int": "node scripts/build-config.js INT",
    "config:prod": "node scripts/build-config.js PROD",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --passWithNoTests",
    "format": "prettier --write .",
    "format:locales": "node scripts/format-locales.js",
    "validate": "node scripts/validate-push.cjs",
    "push": "npm run validate && git push",
    "push:origin": "npm run validate && git push origin",
    "push:main": "npm run validate && git push origin main",
    "push:unsafe": "git push --no-verify",
    "push:unsafe:origin": "git push origin --no-verify",
    "push:unsafe:main": "git push origin main --no-verify",
    "deploy": "npm run build:prod && gh-pages -d dist",
    "prepare": "husky"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "i18next": "^23.7.6",
    "leaflet": "^1.9.4",
    "leaflet-draw": "^1.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.0",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.28.0"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.2",
    "@eslint/js": "^9.33.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.14",
    "@types/leaflet": "^1.9.8",
    "@types/leaflet-draw": "^1.0.13",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^9.33.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "gh-pages": "^6.1.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.3.6",
    "ts-jest": "^29.4.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2"
  }
}
